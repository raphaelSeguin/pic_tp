pipeline {
    agent {
        label 'SLAVE'
    }
    stages {
        stage('use ansible'){
            steps {
                ansiblePlaybook(
                    credentialsId: 'slave',
                    inventory: 'deployment_playbooks/inventory',
                    playbook:  'deployment_playbooks/deployment-playbook.yml'
                )
            }
        }
    }
}
